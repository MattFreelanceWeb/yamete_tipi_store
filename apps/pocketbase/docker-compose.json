{
  "services": [
    {
      "name": "pocketbase",
      "image": "ghcr.io/pocketbase/pocketbase:latest",
      "command": [
        "serve",
        "--dir",
        "/data"
      ],
      "internalPort": 8090,
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/pocketbase",
          "containerPath": "/data",
          "readOnly": false,
          "shared": false,
          "private": false
        }
      ]
    }
  ]
}