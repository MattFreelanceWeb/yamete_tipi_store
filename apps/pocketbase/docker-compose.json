{
  "services": [
    {
      "name": "pocketbase",
      "image": "elestio/pocketbase:v0.22.11",
      "command": "serve --dir /pb_data --http 0.0.0.0:8090",
      "internalPort": 8090,
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/pocketbase",
          "containerPath": "/pb_data",
          "readOnly": false,
          "shared": false,
          "private": false
        }
      ],
      "user": "0:0"
    }
  ]
}
